@model IEnumerable<Book>

@{
    ViewData["Title"] = "Home Page";
}

<h1> All Book List</h1>
<table class=" table table-bordered table-striped" style="width:100%">

    <thead>
        <tr>
            <th>
                Book Name
            </th>
            <th>
                Author Name
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model)
        {
            <tr>
                <td width="50%">
                    @obj.Name
                </td>
                <td width="30%"> 
                    @obj.AuthorName
                </td>

            </tr>
        }
    </tbody>
</table>